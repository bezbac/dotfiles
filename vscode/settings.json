{
  "editor.fontSize": 14,
  "editor.fontFamily": "Fira Code",
  "editor.minimap.renderCharacters": false,
  "editor.minimap.showSlider": "always",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.autoClosingQuotes": "always",
  "editor.quickSuggestions": {
    "strings": true
  },
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "editor.inlineSuggest.enabled": true,
  "terminal.external.osxExec": "iTerm2.app",
  "terminal.integrated.fontSize": 14,
  "workbench.statusBar.feedback.visible": false,
  "workbench.editor.untitled.hint": "hidden",
  "workbench.colorTheme": "Concrete",
  "workbench.startupEditor": "welcomePage",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "window.titleBarStyle": "native",
  "explorer.sortOrderLexicographicOptions": "upper",
  "extensions.ignoreRecommendations": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "git.confirmSync": false,
  "files.exclude": {
    "**/node_modules": true
  },
  // Formatting
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  // Fix emmet in react
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "vue-html": "html"
  },
  "emmet.syntaxProfiles": {
    "javascript": "jsx"
  },
  "jupyter.askForKernelRestart": false,
  "scss.validate": false,
  "tailwindCSS.experimental.classRegex": [
    "tw`([^`]*)", // tw`...`
    "tw=\"([^\"]*)", // <div tw="..." />
    "tw={\"([^\"}]*)", // <div tw={"..."} />
    "tw\\.\\w+`([^`]*)", // tw.xxx`...`
    "tw\\(.*?\\)`([^`]*)" // tw(Component)`...`
  ],
  "tailwindCSS.includeLanguages": {
    "typescript": "javascript",
    "typescriptreact": "javascript"
  },
  // "rust-analyzer.cargo.allFeatures": true,
  "customizeUI.titleBar": "regular",
  "customizeUI.stylesheet": {
    "[id='status.host']": "order: -2;",
    "[id='vscodevim.vim']": "order: -1; margin-left: 2px;",
    "[id='vscodevim.vim'] a": "color: transparent; position: relative; font-weight: 700;",
    "[id='vscodevim.vim'] a::after": "position: absolute; left: 0; text-align: center; width: 100%;",
    "[id='vscodevim.vim'][aria-label='-- NORMAL --']": "background: #586069;",
    "[id='vscodevim.vim'][aria-label='-- NORMAL --'] a::after": "content: 'NORMAL'; color: #EEEEEE;",
    "[id='vscodevim.vim'][aria-label='-- INSERT --']": "background: #85E89D;",
    "[id='vscodevim.vim'][aria-label='-- INSERT --'] a::after": "content: 'INSERT'; color: #202020;",
    "[id='vscodevim.vim'][aria-label='-- VISUAL --']": "background: #B392F0;",
    "[id='vscodevim.vim'][aria-label='-- VISUAL --'] a::after": "content: 'VISUAL'; color: #202020;",
    "[id='asvetliakov.vscode-neovim']": "order: -1; margin-left: 2px;",
    "[id='asvetliakov.vscode-neovim'] a": "color: transparent; position: relative; font-weight: 700; width: 4rem;",
    "[id='asvetliakov.vscode-neovim'] a::after": "position: absolute; left: 0; text-align: center; width: 100%;",
    "[id='asvetliakov.vscode-neovim'][aria-label=':']": "background: #586069;",
    "[id='asvetliakov.vscode-neovim'][aria-label=':'] a::after": "content: 'COMMAND'; color: #EEEEEE;",
    "[id='asvetliakov.vscode-neovim'][aria-label='-- EINFÜGEN --']": "background: #85E89D;",
    "[id='asvetliakov.vscode-neovim'][aria-label='-- EINFÜGEN --'] a::after": "content: 'INSERT'; color: #202020;",
    "[id='asvetliakov.vscode-neovim'][aria-label='-- VISUELL --']": "background: #B392F0;",
    "[id='asvetliakov.vscode-neovim'][aria-label='-- VISUELL --'] a::after": "content: 'VISUAL'; color: #202020;",
    "[id='asvetliakov.vscode-neovim'][aria-label='1']": "display: none !important;"
  },
  "vscode-neovim.neovimExecutablePaths.darwin": "/usr/local/bin/nvim",
  "githubPullRequests.createOnPublishBranch": "never",
  "redhat.telemetry.enabled": false,
  "git.mergeEditor": false,
  "githubPullRequests.pullBranch": "never",
  "githubPullRequests.defaultMergeMethod": "squash",
  "githubPullRequests.ignoredPullRequestBranches": [
    "staging",
    "master",
    "dev"
  ],
  "javascript.updateImportsOnFileMove.enabled": "never"
}